
<form>
    <div class="form-group">
        <label for="Event Title">Event Title</label>
        <input type="text" class="form-control" id="publishEventTitle" placeholder="Event Title">
    </div>
    <div class="form-group">
        <label for="Event Host">Event Host</label>
        <input type="text" class="form-control" id="publishFirstName" placeholder="First Name" />
        <input type="text" class="form-control" id="publishLastName" placeholder="Last Name" />
    </div>
    <div class="form-group">
        <label for="Host Email">Host Contact Email</label>
        <input type="email" class="form-control" id="publishEmail" placeholder="Email">
    </div>
    <div class="form-group">
        <label for="Event Information">Event Information</label>
        <textarea class="form-control" id="publishEventInformation" rows="4"></textarea>
    </div>
    <div class="form-group">
        <label for="Street Address">Street Address</label>
        <input type="text" class="form-control" id="publishStreetAddress" placeholder="Street Address">
    </div>
    <div class="form-group">
        <label for="Address Line 2">Address Line 2</label>
        <input type="text" class="form-control" id="publishAddressLine2" placeholder="">
    </div>
    <div class="form-group">
        <label for="City">City</label>
        <input type="text" class="form-control" id="publishCity" placeholder="City Name">
    </div>
    <div class="form-group">
        <label for="State">State / Province / Region</label>
        <input type="text" class="form-control" id="publishState" placeholder="State Name">
    </div>
    <div class="form-group">
        <label for="State">Postal / Zip Code</label>
        <input type="text" class="form-control" id="publishZip" placeholder="Postal / Zip Code">
    </div>
    <div class="form-group">
        <label for="State">Country</label>
        <input type="text" class="form-control" id="publishCountry" placeholder="Country Name">
    </div>
</form>
