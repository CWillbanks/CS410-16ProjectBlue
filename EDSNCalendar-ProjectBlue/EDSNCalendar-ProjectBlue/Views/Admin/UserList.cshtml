@model List<String>
@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Registerd User List</h2>

<table class="table table-hover table-condensed">
    <thead>
        <tr>
            <th>Username</th>
            <th>Remove</th>
        </tr>
    </thead>
    <tbody>
        @Html.ActionLink("Register new user", "Register", "Account", routeValues: null, htmlAttributes: new { id = "registerLink" })

        @foreach (String str in Model)
        {
            <tr>
                <td>@str.ToString()</td>
                <td>@Html.ActionLink("Remove User", "ConfirmRemoveUser", "Admin", new { s = (str.Replace('@','t')).Replace('.','t') }, null)</td>
            </tr>
        }
    </tbody>
</table>